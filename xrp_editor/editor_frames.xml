					<CheckButton name="$parentCUDefault" parentKey="CUDefault" inherits="TinyEditorCheck">
						<Anchors>
							<Anchor point="LEFT" relativeKey="$parent.CULabel" relativePoint="RIGHT"/>
						</Anchors>
					</CheckButton>
					<!-- Description Box -->
					<ScrollFrame name="$parentDE" parentKey="DE" inherits="LargeInputScrollFrameTemplate">
						<Size x="301" y="163"/>
						<Anchors>
							<Anchor point="TOPLEFT" relativeKey="$parent.DELabel" relativePoint="BOTTOMLEFT" x="2" y="-5"/>
						</Anchors>
					</ScrollFrame>
					<CheckButton name="$parentDEDefault" parentKey="DEDefault" inherits="TinyEditorCheck">
						<Anchors>
							<Anchor point="LEFT" relativeKey="$parent.DELabel" relativePoint="RIGHT"/>
						</Anchors>
					</CheckButton>
				</Frames>
			</Frame>
			<Frame name="$parentBiography" parentKey="Biography" useParentLevel="true" hidden="true">
				<Anchors>
					<Anchor point="TOPLEFT" relativeKey="$parent.Inset" relativePoint="TOPLEFT"/>
					<Anchor point="BOTTOMRIGHT" relativeKey="$parent.Inset" relativePoint="BOTTOMRIGHT"/>
				</Anchors>
				<Layers>
					<Layer level="OVERLAY">
						<!-- Age Label -->
						<FontString name="$parentAGLabel" parentKey="AGLabel" inherits="GameFontNormalSmall" text="XRP_AG">
							<Anchors>
								<Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="12" y="-8"/>
							</Anchors>
						</FontString>
						<!-- Home Label -->
						<FontString name="$parentHHLabel" parentKey="HHLabel" inherits="GameFontNormalSmall" text="XRP_HH">
							<Anchors>
								<Anchor point="LEFT" relativeKey="$parent.AGLabel" relativePoint="LEFT" x="50" y="0"/>
							</Anchors>
						</FontString>
						<!-- Birthplace Label -->
						<FontString name="$parentHBLabel" parentKey="HBLabel" inherits="GameFontNormalSmall" text="XRP_HB">
							<Anchors>
								<Anchor point="LEFT" relativeKey="$parent.HHLabel" relativePoint="LEFT" x="133" y="0"/>
							</Anchors>
						</FontString>
						<!-- Motto Label -->
						<FontString name="$parentMOLabel" parentKey="MOLabel" inherits="GameFontNormalSmall" text="XRP_MO">
							<Anchors>
								<Anchor point="TOPLEFT" relativeKey="$parent.AGLabel" relativePoint="BOTTOMLEFT" x="0" y="-25"/>
							</Anchors>
						</FontString>
						<!-- History Label -->
						<FontString name="$parentHILabel" parentKey="HILabel" inherits="GameFontNormalSmall" text="XRP_HI">
							<Anchors>
								<Anchor point="TOPLEFT" relativeKey="$parent.MOLabel" relativePoint="BOTTOMLEFT" x="0" y="-25"/>
							</Anchors>
						</FontString>
						<!-- Style Label -->
						<FontString name="$parentFRLabel" parentKey="FRLabel" inherits="GameFontNormalSmall" text="XRP_FR">
							<Anchors>
								<Anchor point="TOPLEFT" relativeKey="$parent.HILabel" relativePoint="BOTTOMLEFT" x="0" y="-208"/>
							</Anchors>
						</FontString>
						<!-- Status Label -->
						<FontString name="$parentFCLabel" parentKey="FCLabel" inherits="GameFontNormalSmall" text="XRP_FC">
							<Anchors>
								<Anchor point="LEFT" relativeKey="$parent.FRLabel" relativePoint="LEFT" x="158" y="0"/>
							</Anchors>
						</FontString>
					</Layer>
				</Layers>
				<Frames>
					<!-- Age Box -->
					<EditBox name="$parentAG" parentKey="AG" inherits="FancyInputBoxTemplate" autoFocus="false">
						<Size x="40" y="20"/>
						<Anchors>
							<Anchor point="TOPLEFT" relativeKey="$parent.AGLabel" relativePoint="BOTTOMLEFT" x="2" y="0"/>
						</Anchors>
					</EditBox>
					<CheckButton name="$parentAGDefault" parentKey="AGDefault" inherits="TinyEditorCheck">
						<Anchors>
							<Anchor point="LEFT" relativeKey="$parent.AGLabel" relativePoint="RIGHT"/>
						</Anchors>
					</CheckButton>
					<!-- Home Box -->
					<EditBox name="$parentHH" parentKey="HH" inherits="FancyInputBoxTemplate" autoFocus="false">
						<Size x="123" y="20"/>
						<Anchors>
							<Anchor point="TOPLEFT" relativeKey="$parent.HHLabel" relativePoint="BOTTOMLEFT" x="2" y="0"/>
						</Anchors>
					</EditBox>
					<CheckButton name="$parentHHDefault" parentKey="HHDefault" inherits="TinyEditorCheck">
						<Anchors>
							<Anchor point="LEFT" relativeKey="$parent.HHLabel" relativePoint="RIGHT"/>
						</Anchors>
					</CheckButton>
					<!-- Birthplace Box -->
					<EditBox name="$parentHB" parentKey="HB" inherits="FancyInputBoxTemplate" autoFocus="false">
						<Size x="123" y="20"/>
						<Anchors>
							<Anchor point="TOPLEFT" relativeKey="$parent.HBLabel" relativePoint="BOTTOMLEFT" x="2" y="0"/>
						</Anchors>
					</EditBox>
					<CheckButton name="$parentHBDefault" parentKey="HBDefault" inherits="TinyEditorCheck">
						<Anchors>
							<Anchor point="LEFT" relativeKey="$parent.HBLabel" relativePoint="RIGHT"/>
						</Anchors>
					</CheckButton>
					<!-- Motto Box -->
					<EditBox name="$parentMO" parentKey="MO" inherits="FancyInputBoxTemplate" autoFocus="false">
						<Size x="306" y="20"/>
						<Anchors>
							<Anchor point="TOPLEFT" relativeKey="$parent.MOLabel" relativePoint="BOTTOMLEFT" x="2" y="0"/>
						</Anchors>
					</EditBox>
					<CheckButton name="$parentMODefault" parentKey="MODefault" inherits="TinyEditorCheck">
						<Anchors>
							<Anchor point="LEFT" relativeKey="$parent.MOLabel" relativePoint="RIGHT"/>
						</Anchors>
					</CheckButton>
					<!-- History Box -->
					<ScrollFrame name="$parentHI" parentKey="HI" inherits="LargeInputScrollFrameTemplate">
						<Size x="301" y="193"/>
						<Anchors>
							<Anchor point="TOPLEFT" relativeKey="$parent.HILabel" relativePoint="BOTTOMLEFT" x="2" y="-5"/>
						</Anchors>
					</ScrollFrame>
					<CheckButton name="$parentHIDefault" parentKey="HIDefault" inherits="TinyEditorCheck">
						<Anchors>
							<Anchor point="LEFT" relativeKey="$parent.HILabel" relativePoint="RIGHT"/>
						</Anchors>
					</CheckButton>
					<!-- Style Box -->
					<EditBox name="$parentFR" parentKey="FR" inherits="FancyInputBoxTemplate" autoFocus="false">
						<Size x="148" y="20"/>
						<Anchors>
							<Anchor point="TOPLEFT" relativeKey="$parent.FRLabel" relativePoint="BOTTOMLEFT" x="2" y="-3"/>
						</Anchors>
					</EditBox>
					<CheckButton name="$parentFRDefault" parentKey="FRDefault" inherits="TinyEditorCheck">
						<Anchors>
							<Anchor point="LEFT" relativeKey="$parent.FRLabel" relativePoint="RIGHT"/>
						</Anchors>
					</CheckButton>
					<!-- Status Menu -->
					<Button name="$parentFC" parentKey="FC" inherits="UIDropDownMenuTemplate">
						<Anchors>
							<Anchor point="TOPLEFT" relativeKey="$parent.FCLabel" relativePoint="BOTTOMLEFT" x="-22" y="0"/>
						</Anchors>
						<Scripts>
							<OnLoad>
								UIDropDownMenu_SetWidth(self, 139, 0);
								function self:SetText(value)
									UIDropDownMenu_Initialize(self, self.initialize);
									UIDropDownMenu_SetSelectedValue(self, value);
								end;
								function self:GetText()
									return UIDropDownMenu_GetSelectedValue(self);
								end;
							</OnLoad>
						</Scripts>
					</Button>
					<CheckButton name="$parentFCDefault" parentKey="FCDefault" inherits="TinyEditorCheck">
						<Anchors>
							<Anchor point="LEFT" relativeKey="$parent.FCLabel" relativePoint="RIGHT"/>
						</Anchors>
					</CheckButton>
				</Frames>
			</Frame>
			<!-- Saved Character Profiles -->
			<Button name="$parentProfiles" parentKey="Profiles" inherits="UIDropDownMenuTemplate">
				<Anchors>
					<Anchor point="TOPRIGHT" relativePoint="TOPRIGHT" x="-175" y="-27"/>
				</Anchors>
				<Scripts>
					<OnLoad>
						UIDropDownMenu_SetWidth(self, 100, 0);
						function self:SetText(value)
							UIDropDownMenu_Initialize(self, self.initialize);
							UIDropDownMenu_SetSelectedValue(self, value);
						end;
						function self:GetText()
							return UIDropDownMenu_GetSelectedValue(self);
						end;
					</OnLoad>
				</Scripts>
			</Button>
			<!-- + Profile Button -->
			<Button name="$parentPlusProfile" parentKey="PlusProfile" inherits="UIPanelButtonTemplate" text=" + ">
				<Size x="28" y="28"/>
				<Anchors>
					<Anchor point="LEFT" relativeKey="$parent.Profiles" relativePoint="RIGHT" x="34" y="2"/>
				</Anchors>
				<Scripts>
					<OnClick>
						StaticPopup_Show("XRP_EDITOR_ADD");
					</OnClick>
					<OnEnter>
						GameTooltip:SetOwner(self, "ANCHOR_CURSOR");
						GameTooltip:SetText("Add");
						GameTooltip:Show();
					</OnEnter>
					<OnLeave>
						GameTooltip:Hide();
					</OnLeave>
				</Scripts>
			</Button>
			<!-- - Profile Button -->
			<Button name="$parentMinusProfile" parentKey="MinusProfile" inherits="UIPanelButtonTemplate" text=" - ">
				<Size x="28" y="28"/>
				<Anchors>
					<Anchor point="LEFT" relativeKey="$parent.PlusProfile" relativePoint="RIGHT" x="0" y="0"/>
				</Anchors>
				<Scripts>
					<OnClick>
						StaticPopupDialogs["XRP_EDITOR_DELETE"].text = format("Are you sure you want to remove \"%s\"?", xrp.editor.Profiles:GetText());
						StaticPopup_Show("XRP_EDITOR_DELETE");
					</OnClick>
					<OnEnter>
						GameTooltip:SetOwner(self, "ANCHOR_CURSOR");
						GameTooltip:SetText("Delete");
						GameTooltip:Show();
					</OnEnter>
					<OnLeave>
						GameTooltip:Hide();
					</OnLeave>
				</Scripts>
			</Button>
			<!-- R Profile Button -->
			<Button name="$parentRenameProfile" parentKey="RenameProfile" inherits="UIPanelButtonTemplate" text=" R ">
				<Size x="28" y="28"/>
				<Anchors>
					<Anchor point="LEFT" relativeKey="$parent.MinusProfile" relativePoint="RIGHT" x="0" y="0"/>
				</Anchors>
				<Scripts>
					<OnClick>
						StaticPopupDialogs["XRP_EDITOR_RENAME"].text = format("Please enter a new name for \"%s\".", xrp.editor.Profiles:GetText());
						StaticPopup_Show("XRP_EDITOR_RENAME");
					</OnClick>
					<OnEnter>
						GameTooltip:SetOwner(self, "ANCHOR_CURSOR");
						GameTooltip:SetText("Rename");
						GameTooltip:Show();
					</OnEnter>
					<OnLeave>
						GameTooltip:Hide();
					</OnLeave>
				</Scripts>
			</Button>
			<!-- C Profile Button -->
			<Button name="$parentCopyProfile" parentKey="CopyProfile" inherits="UIPanelButtonTemplate" text=" C ">
				<Size x="28" y="28"/>
				<Anchors>
					<Anchor point="LEFT" relativeKey="$parent.RenameProfile" relativePoint="RIGHT" x="0" y="0"/>
				</Anchors>
				<Scripts>
					<OnClick>
						StaticPopupDialogs["XRP_EDITOR_COPY"].text = format("Please enter a name for the copy of \"%s\".", xrp.editor.Profiles:GetText());
						StaticPopup_Show("XRP_EDITOR_COPY");
					</OnClick>
					<OnEnter>
						GameTooltip:SetOwner(self, "ANCHOR_CURSOR");
						GameTooltip:SetText("Copy");
						GameTooltip:Show();
					</OnEnter>
					<OnLeave>
						GameTooltip:Hide();
					</OnLeave>
				</Scripts>
			</Button>
			<!-- Save Button -->
			<Button name="$parentSave" parentKey="SaveButton" inherits="UIPanelButtonTemplate" text="SAVE">
				<Size x="96" y="21"/>
				<Anchors>
					<Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" x="-6" y="4"/>
				</Anchors>
				<Scripts>
					<OnClick>
						xrp.editor:Save();
					</OnClick>
				</Scripts>
			</Button>
			<!-- Revert Button -->
			<Button name="$parentRevert" parentKey="RevertButton" inherits="UIPanelButtonTemplate" text="Revert">
				<Size x="96" y="21"/>
				<Anchors>
					<Anchor point="RIGHT" relativeKey="$parent.SaveButton" relativePoint="LEFT"/>
				</Anchors>
				<Scripts>
					<OnClick>
						xrp.editor:Revert();
					</OnClick>
				</Scripts>
			</Button>
			<Button name="$parentTab1" parentKey="Tab1" inherits="CharacterFrameTabButtonTemplate" id="1" text="Appearance">
				<Anchors>
					<Anchor point="TOPLEFT" relativePoint="BOTTOMLEFT" x="11" y="2"/>
				</Anchors>
				<Scripts>
					<OnClick>
						PanelTemplates_SetTab(self:GetParent(), 1);
						self:GetParent().Biography:Hide();
						self:GetParent().Appearance:Show();
						PlaySound("igCharacterInfoTab");
					</OnClick>
				</Scripts>
			</Button>
			<Button name="$parentTab2" parentKey="Tab2" inherits="CharacterFrameTabButtonTemplate" id="2" text="Biography">
				<Anchors>
					<Anchor point="LEFT" relativeKey="$parent.Tab1" relativePoint="RIGHT" x="-16" y="0"/>
				</Anchors>
				<Scripts>
					<OnClick>
						PanelTemplates_SetTab(self:GetParent(), 2);
						self:GetParent().Appearance:Hide();
						self:GetParent().Biography:Show();
						PlaySound("igCharacterInfoTab");
					</OnClick>
				</Scripts>
			</Button>
		</Frames>
		<Scripts>
			<OnShow>
				SetPortraitTexture(self.portrait, "player")
				PlaySound("igCharacterInfoOpen");
			</OnShow>
			<OnHide>
				PlaySound("igCharacterInfoClose");
			</OnHide>
		</Scripts>
	</Frame>
</Ui>
