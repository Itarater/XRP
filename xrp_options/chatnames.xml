	<Frame name="$parent_chatnames" parentKey="chatnames" parent="xrp_options">
		<Layers>
			<Layer level="OVERLAY">
				<FontString parentKey="Title" inherits="GameFontNormalLarge" text="XRP_OPTIONS_CHAT_TITLE">
					<Anchors>
						<Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="15" y="-15"/>
					</Anchors>
				</FontString>
				<FontString parentKey="ChatLabel" inherits="GameFontNormal" text="XRP_OPTIONS_CHAT_LABEL">
					<Anchors>
						<Anchor point="TOPLEFT" relativeKey="$parent.Title" relativePoint="BOTTOMLEFT" x="0" y="-10"/>
					</Anchors>
				</FontString>
				<FontString parentKey="SayLabel" inherits="GameFontWhite" text="CHAT_MSG_SAY">
					<Anchors>
						<Anchor point="TOPLEFT" relativeKey="$parent.ChatLabel" relativePoint="BOTTOMLEFT" x="24" y="-10"/>
					</Anchors>
				</FontString>
				<FontString parentKey="YellLabel" inherits="GameFontWhite" text="CHAT_MSG_YELL">
					<Anchors>
						<Anchor point="LEFT" relativeKey="$parent.SayLabel" relativePoint="RIGHT" x="50" y="0"/>
					</Anchors>
				</FontString>
				<FontString parentKey="EmoteLabel" inherits="GameFontWhite" text="CHAT_MSG_EMOTE">
					<Anchors>
						<Anchor point="LEFT" relativeKey="$parent.YellLabel" relativePoint="RIGHT" x="50" y="0"/>
					</Anchors>
				</FontString>
				<FontString parentKey="WhisperLabel" inherits="GameFontWhite" text="CHAT_MSG_WHISPER_INFORM">
					<Anchors>
						<Anchor point="LEFT" relativeKey="$parent.EmoteLabel" relativePoint="RIGHT" x="50" y="0"/>
					</Anchors>
				</FontString>
				<FontString parentKey="GuildLabel" inherits="GameFontWhite" text="CHAT_MSG_GUILD">
					<Anchors>
						<Anchor point="TOPLEFT" relativeKey="$parent.SayLabel" relativePoint="BOTTOMLEFT" x="0" y="-20"/>
					</Anchors>
				</FontString>
				<FontString parentKey="PartyLabel" inherits="GameFontWhite" text="CHAT_MSG_PARTY">
					<Anchors>
						<Anchor point="LEFT" relativeKey="$parent.GuildLabel" relativePoint="RIGHT" x="50" y="0"/>
					</Anchors>
				</FontString>
				<FontString parentKey="RaidLabel" inherits="GameFontWhite" text="CHAT_MSG_RAID">
					<Anchors>
						<Anchor point="LEFT" relativeKey="$parent.PartyLabel" relativePoint="RIGHT" x="50" y="0"/>
					</Anchors>
				</FontString>
				<FontString parentKey="InstanceLabel" inherits="GameFontWhite" text="INSTANCE_CHAT_MESSAGE">
					<Anchors>
						<Anchor point="LEFT" relativeKey="$parent.RaidLabel" relativePoint="RIGHT" x="50" y="0"/>
					</Anchors>
				</FontString>
				<FontString parentKey="ChannelLabel" inherits="GameFontWhite" text="CHAT_MSG_CHANNEL_LIST">
					<Anchors>
						<Anchor point="TOPLEFT" relativeKey="$parent.GuildLabel" relativePoint="BOTTOMLEFT" x="0" y="-20"/>
					</Anchors>
				</FontString>
				<FontString parentKey="MiscLabel" inherits="GameFontNormal" text="XRP_OPTIONS_CHAT_MISC">
					<Anchors>
						<Anchor point="TOPLEFT" relativeKey="$parent.ChatLabel" relativePoint="BOTTOMLEFT" x="0" y="-100"/>
					</Anchors>
				</FontString>
				<FontString parentKey="emotebracedLabel" inherits="GameFontWhite" text="XRP_OPTIONS_CHAT_EMOTEBRACED">
					<Anchors>
						<Anchor point="TOPLEFT" relativeKey="$parent.MiscLabel" relativePoint="BOTTOMLEFT" x="24" y="-10"/>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Frames>
			<CheckButton name="$parentSay" parentKey="CHAT_MSG_SAY" inherits="UICheckButtonTemplate">
				<Size x="24" y="24"/>
				<Anchors>
					<Anchor point="RIGHT" relativeKey="$parent.SayLabel" relativePoint="LEFT" x="-4" y="0"/>
				</Anchors>
			</CheckButton>
			<CheckButton name="$parentYell" parentKey="CHAT_MSG_YELL" inherits="UICheckButtonTemplate">
				<Size x="24" y="24"/>
				<Anchors>
					<Anchor point="RIGHT" relativeKey="$parent.YellLabel" relativePoint="LEFT" x="-4" y="0"/>
				</Anchors>
			</CheckButton>
			<CheckButton name="$parentEmote" parentKey="CHAT_MSG_EMOTE" inherits="UICheckButtonTemplate">
				<Size x="24" y="24"/>
				<Anchors>
					<Anchor point="RIGHT" relativeKey="$parent.EmoteLabel" relativePoint="LEFT" x="-4" y="0"/>
				</Anchors>
			</CheckButton>
			<CheckButton name="$parentGuild" parentKey="CHAT_MSG_GUILD" inherits="UICheckButtonTemplate">
				<Size x="24" y="24"/>
				<Anchors>
					<Anchor point="RIGHT" relativeKey="$parent.GuildLabel" relativePoint="LEFT" x="-4" y="0"/>
				</Anchors>
			</CheckButton>
			<CheckButton name="$parentWhisper" parentKey="CHAT_MSG_WHISPER" inherits="UICheckButtonTemplate">
				<Size x="24" y="24"/>
				<Anchors>
					<Anchor point="RIGHT" relativeKey="$parent.WhisperLabel" relativePoint="LEFT" x="-4" y="0"/>
				</Anchors>
			</CheckButton>
			<CheckButton name="$parentParty" parentKey="CHAT_MSG_PARTY" inherits="UICheckButtonTemplate">
				<Size x="24" y="24"/>
				<Anchors>
					<Anchor point="RIGHT" relativeKey="$parent.PartyLabel" relativePoint="LEFT" x="-4" y="0"/>
				</Anchors>
			</CheckButton>
			<CheckButton name="$parentRaid" parentKey="CHAT_MSG_RAID" inherits="UICheckButtonTemplate">
				<Size x="24" y="24"/>
				<Anchors>
					<Anchor point="RIGHT" relativeKey="$parent.RaidLabel" relativePoint="LEFT" x="-4" y="0"/>
				</Anchors>
			</CheckButton>
			<CheckButton name="$parentInstance" parentKey="CHAT_MSG_INSTANCE" inherits="UICheckButtonTemplate">
				<Size x="24" y="24"/>
				<Anchors>
					<Anchor point="RIGHT" relativeKey="$parent.InstanceLabel" relativePoint="LEFT" x="-4" y="0"/>
				</Anchors>
			</CheckButton>
			<Button name="$parentChannelButton" parentKey="CHANNEL_BUTTON">
				<Size x="24" y="24"/>
				<Anchors>
					<Anchor point="RIGHT" relativeKey="$parent.ChannelLabel" relativePoint="LEFT" x="-4" y="0"/>
				</Anchors>
				<NormalTexture file="Interface\Buttons\UI-SpellbookIcon-NextPage-Up"/>
				<PushedTexture file="Interface\Buttons\UI-SpellbookIcon-NextPage-Down"/>
				<DisabledTexture file="Interface\Buttons\UI-SpellbookIcon-NextPage-Disabled"/>
				<HighlightTexture file="Interface\Buttons\UI-Common-MouseHilight" alphaMode="ADD"/>
			</Button>
			<Button name="$parentChannelMenu" parentKey="CHANNEL_MENU" hidden="true" inherits="UIDropDownMenuTemplate"/>
			<CheckButton name="$parentemotebraced" parentKey="emotebraced" inherits="UICheckButtonTemplate">
				<Size x="24" y="24"/>
				<Anchors>
					<Anchor point="RIGHT" relativeKey="$parent.emotebracedLabel" relativePoint="LEFT" x="-4" y="0"/>
				</Anchors>
			</CheckButton>
		</Frames>
	</Frame>
</Ui>
