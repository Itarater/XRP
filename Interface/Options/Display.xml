					<Anchor point="TOPLEFT" relativeKey="$parent.AltTaurenLimit" relativePoint="BOTTOMLEFT" y="-8"/>
				</Anchors>
				<KeyValues>
					<KeyValue key="dependsOn" value="altTauren"/>
